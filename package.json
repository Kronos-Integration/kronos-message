{
  "name": "@kronos-integration/message",
  "version": "0.0.0-semantic-release",
  "sideEffects": false,
  "type": "module",
  "main": "./src/message.mjs",
  "exports": {
    ".": "./src/message.mjs"
  },
  "description": "The message object is the 'data' object which will be send from step to step",
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/message.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/message.mjs"
  },
  "dependencies": {
    "clone-deep": "^4.0.0",
    "merge-light": "0.0.2",
    "uuid-js": "^0.7.5"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "^13.1.0",
    "semantic-release": "^17.2.2"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/message.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/message/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/message#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-github-action",
      "arlac77/template-kronos-component"
    ]
  }
}
